{
  "presets": [
    ["env", {
      "loose": false,
      "modules": false,
      "spec": true,
      "targets": {
        "browsers": [
          "last 3 Chrome versions",
          "last 3 Firefox versions",
          "Safari >= 10",
          "Explorer >= 9",
          "Edge >= 12",
          "iOS >= 10",
          "Android >= 6"
        ]
      },
      "useBuiltIns": "usage"
    }],
    "stage-2"
  ],
  "plugins": [
    "dynamic-import-node",
    "lodash",
    "transform-class-properties",
    "transform-es5-property-mutators",
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-vue-jsx"
  ],
  "env": {
    "es": {
      "presets": [],
      "plugins": []
    },
    "rollup": {
      "presets": [],
      "plugins": []
    },
    "cjs": {
      "presets": [
        ["env", {
          "modules": "commonjs"
        }],
      ],
      "plugins": []
    }
  }
}
